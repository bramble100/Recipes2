@model Microsoft.EntityFrameworkCore.DbSet<Gamora.Models.Song>

<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>List of Songs</title>
</head>
<body>
    <table>
        <thead>
            <tr>
                <th>Author</th>
                <th>Title</th>
                <th>Year</th>
                <th>Genre</th>
                <th>Rating</th>
            </tr>
        </thead>
        @foreach (var song in Model)
        {
            <tr>
                <td>@song.Author</td>
                <td>@song.Title</td>
                <td>@song.Genre</td>
                <td>@song.Year</td>
                <td>@song.Rating</td>
            </tr>
        }
    </table>
    </body>
</html>